<h2>Detalle {{products.[0].type}}</h2>
<hr>
{{#each products}}
  <div class="card mb-3">
    <img src="{{products[0].imageUrl}}" class="card-img-top" alt="Verduras">
    <div class="card-body">
      <h5 class="card-title">{{this.name}}</h5>
        <ul class="stats">
          <li>Promedio: <span>00</span></li>
          <li>Máximo: <span>00</span></li>
          <li>Mínimo: <span>00</span></li>
        </ul>
      {{#each stores}}
      <table class="table">
        <thead>
          <tr>
          <th scope="col" colspan="2">{{this.storeName}}</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>Precio Profeco:<br>{{this.priceProfeco}}</td>
            <td>Precio Usuario:<br>{{this.priceUser.[0].score}}</td>
          </tr>
        </tbody>
      </table>
      <form action="/updatePriceUser/{{../_id}}/{{this.storeName}}" method="post">
        <label for="score">Actualiza el precio:</label>
        <input type="number" name="score">
        <button type="submit">Ir</button>
      </form>
      {{/each}}
    </div>
  </div>
{{/each}}